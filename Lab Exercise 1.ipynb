   "source": [
    "# Question 2\n",
    "# Change the code below\n",
  i = 1

while i <= 10:
    if i == 3:
        i += 1
        continue  # Skip the number 3
    print(i, end=" ")  # Print the number with a space
    i += 1
   ]
